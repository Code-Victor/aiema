{
  "expo": {
    "name": "Aiema",
    "slug": "aiema",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.aiema.client"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-secure-store",
      [
        "expo-font",
        {
          "fonts": [
            "node_modules/@expo-google-fonts/urbanist/Urbanist_400Regular",
            "node_modules/@expo-google-fonts/urbanist/Urbanist_500Medium",
            "node_modules/@expo-google-fonts/urbanist/Urbanist_600SemiBold",
            "node_modules/@expo-google-fonts/urbanist/Urbanist_700Bold"
          ]
        }
      ],
      [
        "react-native-edge-to-edge",
        {
          "android": {
            "parentTheme": "Light",
            "enforceNavigationBarContrast": false
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "1adea63e-4baf-4085-bfe0-787e699cf6bd"
      }
    },
    "owner": "oluwaborihamzat"
  }
}
